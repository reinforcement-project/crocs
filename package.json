{
  "name": "scratch",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node ./server_dist/server.js",
    "dev": "concurrently \"cross-env NODE_ENV='development' webpack-dev-server --history-api-fallback \" \"nodemon\"",
    "build": "NODE_ENV='production' webpack",
    "build:server": "NODE_ENV=production tsc --project tsconfig.server.json",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx}\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx}\" --quiet",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "test:client": "jest",
    "test:server": "jest -c ./jest-server.config.ts",
    "test": "npm run test:client && npm run test:server",
    "test:coverage": "jest --coverage",
    "check-types": "tsc",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run test && npm run test:e2e:run",
    "prepare": "husky install"
  },
